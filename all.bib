@Article{sorbjan--decay,
  Title                    = {DECAY OF CONVECTIVE TURBULENCE REVISITED},
  Author                   = {Sorbjan, Zbigniew},
  Journal                  = {Boundary-Layer Meteorology},
  Number                   = {3},
  Pages                    = {503--517},
  Volume                   = {82},

  Abstract                 = {Results of a large-eddy simulation of a decaying convective mixed layer over land are presented. The time evolution of the mixed layer is forced by the surface heat flux gradually decreasing with time. The results obtained show that the decay of the turbulent kinetic energy is governed by two scales, the external time scale controlling the surface heat flux changes, and the convective time scale. During the simulation, large eddies persist even when the heat flux at the surface becomes negative. A decoupled residual layer of active turbulence is developed above the stable surface layer. The residual layer is marked by large-scale updrafts that are able to penetrate the capping inversion layer and induce entrainment.},
  Doi                      = {10.1023/A:1000231524314},
  File                     = {:home/nldias/Dropbox/epapers/sorbjan--decay.pdf:PDF},

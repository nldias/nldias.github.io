@Article{pasricha.sing.ea--characteristics,
  Title                    = {Characteristics of atmospheric turbulence in the surface layer over Antarctica},
  Author                   = {Pasricha, P. K. and Singh, R. and Sarkar, S. K. and Dutta, H. N. and Reddy, B. M. and Das, P. K.},
  Journal                  = {Boundary-Layer Meteorology},
  Number                   = {3},
  Pages                    = {207--217},
  Volume                   = {57},

  Abstract                 = {This paper presents meteorological measurements made during the antarctic summer period, on two 9 m and 3 m towers, on the rocky and ice shelf terrains of the Indian antarctic stations Maitri and Dakshin Gangotri, respectively. The measurements of fluctuations in temperature and wind speed made with relatively lesser precision instrumentation pertain to smaller wave numbers {\textasciitilde}10-2 m-1 appropriate to outer scale L 0 of the atmospheric turbulence spectrum. Autocorrelation analysis of the fluctuations in temperature and wind speed has been performed. A new autoregressive scheme has been developed to represent the computed autocorrelation functions by a Yule statistical model, and to estimate the correlation period T 0 of the turbulent medium. Height profiles of outer scale L 0 of turbulence may be given in terms of T 0 and mean wind speed u. Further, the similarity theory of Monin-Obukhov has been used to compute height profiles of temperature structure parameter C T 2. At Maitri, values of L 0 and C T 2 are higher between 03--22 h local time than between 22--03 h. Values of L 0 and C T 2 are smaller over the ice shelf terrain of the Dakshin Gangotri station, compared to those over the rocky terrain of the Maitri station.},
  Doi                      = {10.1007/BF00120885},
  File                     = {:Dropbox/epapers/pasricha.sing.ea--characteristics.pdf:PDF},
  ISSN                     = {1573-1472},
  Url                      = {http://dx.doi.org/10.1007/BF00120885}
}


@Article{mcbean.elliott--pressure.humidity,
  Title                    = {Pressure and humidity effects on optical refractive-index fluctuations},
  Author                   = {McBean, G. A. and Elliott, J. A.},
  Journal                  = {Boundary-Layer Meteorology},
  Number                   = {1},
  Pages                    = {101--109},
  Volume                   = {20},

  Abstract                 = {Although temperature fluctuations dominate the variance of optical refractive index fluctuations it has been shown recently that humidity fluctuations can also be important (e.g., Friche et al., 1975). This paper reports on simultaneous measurements of temperature, humidity and pressure so that the relative importance of all three can be investigated. For the dry site where the measurements were made, the humidity contribution was less than other investigators had found. The major contribution of the pressure fluctuations was through their covariance with temperature, but this term was found to be between 0.03 and 0.4\% of the total variance. The results thus confirmed that pressure fluctuations can be neglected in most circumstances. Both the temperature and humidity spectra displayed -5/3 power laws at small scales while the temperature-humidity cospectrum decreased more rapidly than a -5/3 power law. The temperature-pressure cospectrum decreased even more rapidly than the temperature-humidity cospectrum. The temperature-pressure correlation coefficient was found to be about -0.1. The humidity-pressure correlation was typically between {\textpm}0.05 and the cospectrum poorly defined.},
  Doi                      = {10.1007/BF00119926},
  File                     = {:home/nldias/Dropbox/epapers/mcbean.elliott--pressure.humidity.pdf:PDF},
  ISSN                     = {1573-1472},
  Url                      = {http://dx.doi.org/10.1007/BF00119926}
}

